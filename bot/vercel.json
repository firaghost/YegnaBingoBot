{
  "version": 2,
  "name": "yegna-bingo-bot",
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/webhook.js"
    }
  ],
  "ignoreCommand": "bash -c 'git diff HEAD^ HEAD --quiet -- bot/ || exit 0; exit 1'"
}
