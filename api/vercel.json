{
  "version": 2,
  "name": "yegna-bingo-bot-api",
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/webhook.js"
    },
    {
      "src": "/start-game",
      "dest": "/start-game.js"
    },
    {
      "src": "/check-countdown",
      "dest": "/check-countdown.js"
    },
    {
      "src": "/super-admin-auth",
      "dest": "/super-admin-auth.js"
    },
    {
      "src": "/admin-auth",
      "dest": "/admin-auth.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
  }
}
