{
  "name": "BingoX-bingo-BingoX",
  "version": "1.0.0",
  "description": "Telegram-based bingo game system with Supabase backend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:socket": "tsx server/socket-server.ts",
    "dev:waiting-room": "tsx server/waiting-room-app.ts",
    "dev:game-server": "tsx server/integrated-game-server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:socket\" \"npm run bot\"",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:waiting-room\" \"npm run bot\"",
    "dev:complete": "concurrently \"npm run dev\" \"npm run dev:game-server\" \"npm run bot\"",
    "build": "next build",
    "start": "next start",
    "start:socket": "NODE_ENV=production tsx server/railway-production-server.ts",
    "start:waiting-room": "NODE_ENV=production tsx server/waiting-room-app.ts",
    "start:game-server": "NODE_ENV=production tsx server/integrated-game-server.ts",
    "test:waiting-room": "tsx test/waiting-room-test.ts",
    "test:waiting-room-simple": "tsx test/simple-waiting-room-test.ts",
    "test:ingame": "tsx test/ingame-test-suite.ts",
    "test:atomic-winner": "tsx test/atomic-winner-test.ts",
    "test:atomic-unit": "tsx test/atomic-winner-unit-test.ts",
    "test:backend": "tsx scripts/run-all-tests.ts",
    "test:all": "tsx scripts/run-all-tests.ts",
    "test:generate": "tsx scripts/testsprite-runner.ts",
    "lint": "next lint",
    "bot": "tsx bot/index.ts"
  },
  "keywords": [
    "telegram",
    "bingo",
    "bot",
    "supabase",
    "next.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lucide-react": "^0.344.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.4.1",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwind-merge": "^2.2.1",
    "telegraf": "^4.15.0",
    "tsx": "^4.7.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0"
  }
}
